{
  "globals": {
    "FB": true,
    "ENV": true,
    "IS_PROD": true,
    "BASE_URL": true,
    "QUIZ": true,
    "FB_CLIENT_ID": true
  },
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true
  },
  "extends": ["plugin:vue/recommended"],
  "plugins": ["vue"],
  "rules": {
    "no-param-reassign": ["error", {
      "props": true,
      "ignorePropertyModificationsFor": [
        "theState",
        "state",
        "acc",
        "e"
      ]
    }]
  }
}
